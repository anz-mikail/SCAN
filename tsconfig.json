{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["dom", "es2015"],
    "typeRoots": ["./node_modules/@types"],
    "types": ["react", "react-dom"],

    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "node_modules"
  ]
}